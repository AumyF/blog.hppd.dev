    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx,mdx}\"",
    "lint": "eslint \"src/**/*.tsx\"",
    "tsc": "tsc",
    "start": "export GATSBY_CODEGEN=0 && npm run develop",
    "start-codegen": "export GATSBY_CODEGEN=1 && npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest"
  },
  "dependencies": {
