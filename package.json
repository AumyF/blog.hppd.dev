    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-source-filesystem": "^2.3.24",
    "jest": "^25.5.4",
    "lodash": "^4.17.20",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "ts-jest": "^25.5.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.159",
    "@types/mdx-js__react": "^1.5.2",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.47",
