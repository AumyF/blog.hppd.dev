    "prettier": "^2.0.5",
    "tailwindcss": "^1.7.3",
    "ts-node": "^9.0.0",
    "twin.macro": "^1.7.0",
    "typescript": "^4.0.2"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
